{
  "api": {
    "uri": "localhost",
    "port": 8080,
    "vhost": "localhost",
    "cors": {
      "origin": [
        "*"
      ],
      "maxAge": 86400,
      "headers": [
        "Accept",
        "Authorization",
        "Content-Type",
        "If-None-Match"
      ],
      "credentials": false
    },
    "prefix": "/"
  },
  "db": {
    "name": "lernacats",
    "host": "localhost",
    "port": "5432",
    "username": "postgres",
    "password": "postgres",
    "dialect": "postgres"
  },
  "logger": {
    "timestamp": true,
    "level": "debug",
    "transports": {
      "console": {
        "enabled": true,
        "config": {
          "colorize": true,
          "timestamp": true
        }
      }
    }
  }
}
